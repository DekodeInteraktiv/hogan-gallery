{
  "name": "dekodeinteraktiv/hogan-gallery",
  "description": "Image Gallery Module for Hogan",
  "homepage": "https://github.com/dekodeinteraktiv/hogan-gallery",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.2",
    "dekodeinteraktiv/hogan-core": ">=1.1.7"
  },
  "archive": {
    "exclude": [
      ".editorconfig",
      ".eslintrc",
      ".gitignore",
      ".stylelintrc",
      ".travis.yml",
      "composer.json",
      "composer.lock",
      "LICENSE",
      "phpcs.xml",
      "yarn.lock"
    ]
  },
  "require-dev": {
    "dekodeinteraktiv/coding-standards": "^1.2.0"
  },
  "scripts": {
    "test": [
      "@composer install",
      "./vendor/bin/phpcs"
    ]
  }
}
