{
  "name": "hogan-gallery",
  "version": "1.0.0",
  "private": true,
  "entry": {
    "hogan-photoswipe": "./src/shared/photoswipe.js",
    "hogan-gallery-grid": [
      "./src/grid/grid.js",
      "./src/grid/grid.scss"
    ],
    "hogan-gallery-slider": [
      "./src/slider/slider.js",
      "./src/slider/slider.scss"
    ]
  },
  "scripts": {
    "build": "heisenberg-scripts build",
    "watch": "heisenberg-scripts build --watch"
  },
  "browserslist": [
    "> 1%",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 8",
    "Android >= 4.4",
    "last 2 versions"
  ],
  "heisenberg": {
    "build": {
      "commonsChunkPlugin": false,
      "dest": "assets",
      "hashFilenames": false,
      "manifest": false
    }
  },
  "devDependencies": {
    "heisenberg-scripts": "0.3.8"
  },
  "dependencies": {}
}
